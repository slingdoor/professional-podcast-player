{
  "name": "professional-podcast-player",
  "version": "1.0.0",
  "description": "A modern, professional podcast player built with Next.js, TypeScript, and Tailwind CSS",
  "private": true,
  "author": "slingdoor",
  "license": "MIT",
  "homepage": "https://github.com/slingdoor/professional-podcast-player",
  "repository": {
    "type": "git",
